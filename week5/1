program ex1016

 integer::Lat,Lon
 Real::I,J,Mag,K,L
 Open(1,file='1999.lis',status='old')
 22 read(1,'(18x,i2,F5.2,i3,F5.2,29x,F3.1)',end=23) Lat, I, lon, J, Mag
 write(*,*)Lat+I/60.0, lon+J/60.0, Mag
goto 22
23 close(1)
 Open(2,file='tw_coast.txt',status='old')
 12 read(1,'(F12.8,2x,F11.8)',end=13) K, L
 write(*,*) K, L
goto 12
13 close(1)



end
